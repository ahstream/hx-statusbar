  "license": "UNLICENSED",
  "main": "dist/index.js",
  "scripts": {
    "dev": "webpack serve --open --config webpack/webpack.dev.js",
    "dev:new": "npm run clean:install && npm run dev",
    "build": "webpack --config webpack/webpack.prod.js",
    "build:new": "npm run clean:install && npm run build",
    "build:publish": "npm run patch && npm run build && npm run publish",
    "patch": "npm version patch",
    "patch:commit": "npm run patch && git commit -m 'Patch version'",
    "publish": "npm publish --access public",
    "clean": "rm -rf node_modules",
    "clean:install": "npm run clean && npm install",
    "test": "jest",
    "lint": "eslint --ext .js --max-warnings=0 .",
    "prettier": "prettier --write",
    "wip": "git add . && git commit -m \"wip\"",
    "null": ""
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/eslint-parser": "^7.22.9",
    "@babel/plugin-transform-runtime": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.8.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-webpack-plugin": "^3.2.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "prettier": "^2.7.1",
    "sass": "^1.64.2",
    "sass-loader": "^13.3.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.6"
  }
}
